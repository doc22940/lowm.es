{
  "version": 2,
  "alias": ["lowm.es", "lowmess-shortener"],
  "redirects": [
    {
      "source": "/stats",
      "destination": "https://stats.lowmess.com",
      "permanent": true
    },
    {
      "source": "/records",
      "destination": "https://most-wanted.lowmess.com",
      "permanent": true
    },
    {
      "source": "/cv",
      "destination": "https://resume.lowmess.com",
      "permanent": true
    },
    {
      "source": "/patterns",
      "destination": "https://hero-patterns.lowmess.com",
      "permanent": true
    },
    {
      "source": "/dotfiles",
      "destination": "https://github.com/lowmess/dotfiles",
      "permanent": true
    },
    {
      "source": "/repo",
      "destination": "https://github.com/lowmess/lowmess.com",
      "permanent": true
    },
    {
      "source": "/repos",
      "destination": "https://github.com/lowmess",
      "permanent": true
    },
    {
      "source": "/tweets",
      "destination": "https://twitter.com/lowmess",
      "permanent": true
    },
    {
      "source": "/pay",
      "destination": "https://cash.app/$lowmess",
      "permanent": true
    },
    {
      "source": "/b/(.*)",
      "destination": "https://lowmess.com/blog/$1",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "destination": "https://lowmess.com/$1",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "https://lowmess.com",
      "permanent": true
    }
  ]
}
